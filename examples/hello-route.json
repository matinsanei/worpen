{
  "name": "Hello World API",
  "description": "Simple hello world endpoint",
  "path": "/api/custom/hello",
  "method": "GET",
  "logic": [
    {
      "set": {
        "var": "greeting",
        "value": "Hello from Dynamic Route Engine!"
      }
    },
    {
      "return": {
        "value": {
          "message": "{{greeting}}",
          "timestamp": "2025-12-17",
          "status": "success"
        }
      }
    }
  ],
  "parameters": [],
  "enabled": true
}
