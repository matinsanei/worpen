{
  "id": "4e728e4a-7d48-416b-96b6-92a1094d55d9",
  "name": "benchmark_logic",
  "description": "",
  "path": "/api/benchmark/logic",
  "method": "POST",
  "logic": [
    {
      "set": {
        "var": "price",
        "value": "{{request.payload.price}}"
      }
    },
    {
      "set": {
        "var": "quantity",
        "value": "{{request.payload.quantity}}"
      }
    },
    {
      "math_op": {
        "operation": "multiply",
        "args": [
          "{{price}}",
          "{{quantity}}"
        ]
      }
    },
    {
      "set": {
        "var": "subtotal",
        "value": "{{math_result}}"
      }
    },
    {
      "if": {
        "condition": "{{subtotal}} > 1000",
        "then": [
          {
            "math_op": {
              "operation": "multiply",
              "args": [
                "{{subtotal}}",
                0.9
              ]
            }
          },
          {
            "set": {
              "var": "total",
              "value": "{{math_result}}"
            }
          }
        ],
        "otherwise": null
      }
    },
    {
      "return": {
        "value": {
          "final_price": "{{total}}"
        }
      }
    }
  ],
  "parameters": [],
  "response_schema": null,
  "auth_required": false,
  "rate_limit": null,
  "enabled": true,
  "version": "1.0.0",
  "created_at": "2025-12-30T16:10:13.058898+00:00",
  "updated_at": "2025-12-30T16:10:13.058898+00:00",
  "created_by": "system"
}